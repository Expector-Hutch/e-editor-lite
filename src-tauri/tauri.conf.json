{
  "build": {
    "beforeDevCommand": "yarn run dev",
    "beforeBuildCommand": "yarn run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1234 "
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "e-editor-lite",
  "version": "0.0.0",
  "identifier": "com.expector.e-editor-lite",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "e-editor-lite",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}